install.packages("RSlelnium")
library(RSelenium)
install.packages("rvest")
library(rvest)
remDr <- remoteDriver(
  remoteServerAddr = "localhost",
  port = 4444L,
  browserName = "chrome"
)

remDr$open()
remDr$navigate("https://www.kofia.or.kr/brd/m_96/list.do")
